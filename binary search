#include <bits/stdc++.h>

using namespace std;

int binarySearch(int arr[], int k, int n) {
    int s = 0;
    int e = n - 1;
    while (s <= e) {
        int mid = s + (e - s) / 2;
        if (arr[mid] == k)
            return mid;

        if (arr[mid] < k)
            s = mid + 1;
        else
            e = mid - 1;  
    }
    return -1;
}

int main() {
    int n;
    cin >> n;

    int arr[n];
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    int k;
    cin >> k;
    cout << binarySearch(arr, k, n);

    return 0;
}
